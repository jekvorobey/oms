{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:maxim.kyprov/laravel-health-check.git"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-common-ms-code.git"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-pim-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-message-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-marketing-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-merchant-management-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-store-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-logistics-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-cm-lib"
        },
        {
            "type": "vcs",
            "url": "git@git.yiilab.com:ibt.studio/ibt-cms-lib"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "darkaonline/l5-swagger": "5.8.*",
        "fideloper/proxy": "^4.0",
        "iwengo/ibt-cm-lib": "dev-feature/IBT-451-hold-bonuses as dev-master",
        "iwengo/ibt-cms-lib": "dev-master-ylab as dev-master",
        "iwengo/ibt-common-lib": "dev-master-ylab as dev-master",
        "iwengo/ibt-logistics-lib": "dev-master-ylab as dev-master",
        "iwengo/ibt-marketing-lib": "dev-master-ylab as dev-master",
        "iwengo/ibt-merchant-management-lib": "dev-master-ylab as dev-master",
        "iwengo/ibt-message-lib": "dev-master-ylab as dev-master",
        "iwengo/ibt-pim-lib": "dev-master-ylab as dev-master",
        "iwengo/ibt-store-lib": "dev-master-ylab as dev-master",
        "jenssegers/date": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "letsgoi/laravel-health-check": "dev-master",
        "mikehaertl/php-pdftk": "^0.8.1",
        "phpoffice/phpword": "^0.17.0",
        "sentry/sentry-laravel": "^2.7",
        "spatie/calendar-links": "^1.4",
        "yoomoney/yookassa-sdk-php": "^2.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "guidocella/eloquent-populator": "^2.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "slevomat/coding-standard": "^7.0",
        "spatie/ray": "^1.22",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpunit": "./vendor/bin/phpunit",
        "phpcs": "./vendor/bin/phpcs --standard=phpcs.xml",
        "phpcbf": "./vendor/bin/phpcbf --standard=phpcs.xml"
    }
}
